{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "files": {
            "matchCondition": {
                "route": "{file}",
                "methods": [
                    "GET"
                ]
            },
            "backendUri": "%proxyFiles%"
        },
        "index": {
            "matchCondition": {
                "route": "/",
                "methods": [
                    "GET"
                ]
            },
            
            "backendUri": "%proxyIndex%"
        }
    }
}